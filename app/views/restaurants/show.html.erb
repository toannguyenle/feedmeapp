<h1>Restaurant</h1>
<strong>Name</strong>  <%= @restaurant.name %><br>
<strong>Website</strong> <%= @restaurant.website %><br>
<strong>Phone</strong> <%= @restaurant.phone_number %><br>
<strong>Address</strong> <%= @restaurant.address %><br>
<strong>Yelp ID</strong> <%= @restaurant.yelp_id %><br>
<strong>Image</strong> <%= @restaurant.image_url %><br>
<strong>Categories</strong> <%= @restaurant.categories %><br>
<strong>Lat</strong> <%= @restaurant.lat %><br>
<strong>Long</strong> <%= @restaurant.lng %><br>

<%= link_to 'Edit', edit_restaurant_path(@restaurant) %><br>
<%= link_to 'Destroy', @restaurant, method: :delete%><br>
<%= link_to 'New Restaurant', new_restaurant_path %><br>
<%= link_to 'Restaurants', restaurants_path %><br>
