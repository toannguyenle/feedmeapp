<!DOCTYPE html>
<html>
<head>
  <title>feedme</title>
  <%= stylesheet_link_tag    "application", media: "all"%>
  <%= javascript_include_tag "application"%>
  <%= csrf_meta_tags %>
  </script>
  <!--<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&libraries=places"></script>-->
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-glyphicons.css" rel="stylesheet">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  <link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
</head> 
<body >

<!--   
  ng-app="feedmeApp" manifest="assets/javascripts/angular/app.js"
  <a ui-sref="home">home</a>
  <a ui-sref="products">products</a>
  <a ui-sref="order">order</a>
  <a ui-sref="maps">maps</a> -->


<div id="container">
  <div id="user_header">
  <% if current_user %>
    Logged in as <%= link_to current_user.email, user_path(current_user) %>.
    <%= link_to "Log Out", logout_path %>
  <% else %>
  <div class="col-md-3 col-sm-3 col-xs-3">
    <%=link_to 'Home', root_path %>
  </div>
  <div class="col-md-3 col-sm-3 col-xs-3">
    <%=link_to 'Log in', login_path %> 
   </div>
  <div class="col-md-3 col-sm-3 col-xs-3">
    <%=link_to 'Sign Up', signup_path%>
   </div>
  <div class="col-md-3 col-sm-3 col-xs-3">
    <%=link_to 'Business Sign Up', new_business_path%>
   </div>
  <% end %>
  <p><%= notice %></p>
  </div>
</div>



<%= yield %>

</body>
</html>