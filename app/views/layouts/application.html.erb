<!DOCTYPE html>
<html>
<head>
  <title>feedme</title>
  <%= stylesheet_link_tag    "application", media: "all"%>
  <%= javascript_include_tag "application"%>
  <%= csrf_meta_tags %>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
  <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-glyphicons.css" rel="stylesheet">
</head>
<body ng-app="feedmeApp">

  <a ui-sref="home">home</a>
  <a ui-sref="options">options</a>
  <a ui-sref="order">order</a>
  <a ui-sref="newOrder">new order</a>
  <a ui-sref="product">product</a>
  <a ui-sref="newProduct">new product</a>
  <a ui-sref="user">user</a>
  <a ui-sref="restaurant">restaurant</a>
  <a ui-sref="newRestaurant">new resto</a>
  
<%= yield %>

</body>
</html>
